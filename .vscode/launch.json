    "version": "0.2.0",
    "configurations": [
        {
            "name": "gcc.exe - 运行gcc输出文件",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false, //控制台输出,false则不显示终端窗口
            "MIMode": "gdb",
            "miDebuggerPath": "D:/mingw64/bin/gdb.exe", //修改成你自己的路径
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "gcc.exe build active file" 
            //该处一定要与tasks.json的中编译任务的lable一致,因为必须先编译后运行
        },
        {
            "type": "java",
            "name": "CodeLens (Launch) - Solution",
            "request": "launch",
            "mainClass": "nowcoder.Solution",
            "projectName": "VSCodeProjects_101a0006"
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        }
    ]
}