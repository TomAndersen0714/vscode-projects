{
    "core": {
        "data_dir": "./data",
        "log_dir": "./log"
    },
    "connection": {
        "defaults": {
            "source": {
                "pulsar": {
                    "timeout_millis": 15000,
                    "concurrency": 1
                }
            },
            "sink": {
                "impala": {
                    "buffer_flush_interval_secs": 30
                },
                "clickhouse": {
                    "buffer_flush_interval_secs": 30
                }
            }
        },
        "source": {
            "mini_send_tb": {
                "type": "pulsar",
                "url": "pulsar://pulsar-cluster01-slb:6650",
                "topic": "persistent://bigdata/data_cross/mini_send_tb",
                "subscription": "bigdata_data_sync",
                "timeout_millis": 15000,
                "concurrency": 1
            },
            "ks_send_tb": {
                "type": "pulsar",
                "url": "pulsar://pulsar-cluster01-slb:6650",
                "topic": "persistent://bigdata/data_cross/ks_send_tb",
                "subscription": "bigdata_data_sync",
                "timeout_millis": 15000,
                "concurrency": 1
            },
            "jd_send_tb": {
                "type": "pulsar",
                "url": "pulsar://pulsar-cluster01-slb:6650",
                "topic": "persistent://bigdata/data_cross/jd_send_tb",
                "subscription": "bigdata_data_sync",
                "timeout_millis": 15000,
                "concurrency": 1
            },
            "yz_send_tb": {
                "type": "pulsar",
                "url": "pulsar://pulsar-cluster01-slb:6650",
                "topic": "persistent://bigdata/data_cross/yz_send_tb",
                "subscription": "bigdata_data_sync",
                "timeout_millis": 15000,
                "concurrency": 1
            }
        },
        "sink": {
            "clickhouse_tb": {
                "type": "clickhouse",
                "hosts": "10.0.2.1",
                "ports": 19000,
                "username": None,
                "password": None,
                "buffer_flush_interval_secs": 30
            },
            "impala_tb": {
                "type": "impala",
                "hosts": "10.0.2.1",
                "ports": 21050,
                "username": None,
                "password": None,
                "buffer_flush_interval_secs": 30
            }
        }
    }
